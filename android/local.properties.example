# Local Properties for Mubitt Development
# Copy this file to local.properties and fill in your actual values
# NEVER commit local.properties to version control

# Google Maps API Key (get from Google Cloud Console)
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Firebase configuration
FIREBASE_PROJECT_ID=mubitt-san-juan

# Keystore configuration for release builds
KEYSTORE_PATH=../keystore/mubitt-release.keystore
KEYSTORE_PASSWORD=your_keystore_password
KEY_ALIAS=mubitt
KEY_PASSWORD=your_key_password

# Backend URLs
DEV_BASE_URL=https://dev-api.mubitt.com/
STAGING_BASE_URL=https://staging-api.mubitt.com/
PROD_BASE_URL=https://api.mubitt.com/